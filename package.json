    "sass": "1.43.4",
    "selector-action": "1.2.0",
    "semver": "7.3.5",
    "terser-webpack-plugin": "5.2.5",
    "ts-pnp": "1.2.0",
    "web-vitals": "2.1.2",
    "webpack-manifest-plugin": "4.0.2",
    "workbox-webpack-plugin": "6.4.1"
  },
  "scripts": {
    "build": "webpack",
    "build:staging": "MONETR_ENV=staging yarn build:production",
    "build:production": "yarn build --mode production",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "clean": "rm -rf build/* || true",
    "start": "MONETR_ENV=${MONETR_ENV:=local} webpack serve --hot --mode development",
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom=ui/**/*.{js,jsx,ts,tsx} --coverageDirectory=./reports"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@hot-loader/react-dom": "17.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.2",
    "@tailwindcss/postcss7-compat": "npm:@tailwindcss/postcss7-compat",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@types/react-google-recaptcha": "2.1.2",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.2",
    "autoprefixer": "10.4.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.3",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "6.5.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "http-status-codes": "2.1.4",
    "jest": "26.6.3",
    "jest-mock-axios": "4.4.1",
    "node-sass": "6.0.1",
    "postcss": "8.3.11",
    "postcss-loader": "6.2.0",
    "prettier": "2.4.1",
    "react-refresh": "0.11.0",
    "react-refresh-typescript": "2.0.2",
    "redoc-cli": "0.13.0",
    "redux-devtools-extension": "2.13.9",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-loader": "9.2.6",
    "typescript": "4.5.2",
    "url-loader": "4.1.1",
    "webpack": "5.64.2",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/ui"
    ],
    "collectCoverageFrom": [
      "ui/**/*.{js,jsx,ts,tsx}",
      "!ui/**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/testSetup.js",
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/ui/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/ui/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/ui/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/ui"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
