{
  "name": "monetr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rspack build",
    "build:staging": "MONETR_ENV=staging rspack build --mode production",
    "build:production": "yarn build --mode production",
    "build-dev": "rspack --mode development",
    "build-prod": "rspack --mode production",
    "clean": "make clean",
    "start": "MONETR_ENV=${MONETR_ENV:=local} rspack serve --mode development",
    "test": "jest",
    "coverage": "jest --coverage --coverageDirectory=./reports",
    "storybook": "storybook dev -p 6006 --disable-telemetry",
    "build-storybook": "storybook build --disable-telemetry"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      ".+/pkg/.+"
    ],
    "modulePaths": [
      "<rootDir>/ui"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|lottie.json|xlsx)$": "<rootDir>/ui/testutils/mocks/fileMock.js"
    },
    "resetMocks": false,
    "collectCoverageFrom": [
      "ui/**/*.{js,jsx,ts,tsx}",
      "!ui/**/*.d.ts",
      "!ui/**/*.stories.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/ui/setupTests.js"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {
    "@date-io/moment": "2.16.1",
    "@ebay/nice-modal-react": "1.2.10",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.0",
    "@mui/lab": "5.0.0-alpha.114",
    "@mui/material": "5.11.3",
    "@mui/styles": "5.11.2",
    "@mui/x-date-pickers": "5.0.20",
    "@sentry/browser": "6.19.7",
    "@sentry/react": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "axios": "0.27.2",
    "camelcase": "6.3.0",
    "classnames": "2.3.2",
    "clsx": "1.2.1",
    "formik": "2.2.9",
    "fs-extra": "10.1.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "notistack": "3.0.1",
    "path-to-regexp": "6.2.1",
    "prompts": "2.4.2",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "2.1.0",
    "react-infinite-scroll-hook": "4.0.4",
    "react-plaid-link": "3.3.2",
    "react-query": "3.39.3",
    "react-router-dom": "6.6.1",
    "react-select": "5.5.9",
    "resolve": "1.22.1",
    "resolve-url-loader": "5.0.0",
    "rrule": "2.7.2",
    "tailwindcss": "3.3.3",
    "web-vitals": "2.1.4",
    "zustand": "4.1.4"
  },
  "devDependencies": {
    "@rspack/binding-linux-x64-gnu": "0.2.7",
    "@rspack/cli": "0.2.7",
    "@storybook/addon-coverage": "0.0.8",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.18",
    "@storybook/addon-styling": "1.0.8",
    "@storybook/addon-viewport": "7.0.18",
    "@storybook/blocks": "7.0.18",
    "@storybook/react": "7.0.18",
    "@storybook/test-runner": "0.10.0",
    "@storybook/testing-library": "0.0.14-next.2",
    "@swc/core": "1.3.40",
    "@swc/jest": "0.2.26",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.5",
