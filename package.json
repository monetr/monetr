    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "css-loader": "6.7.3",
    "dotenv": "16.0.3",
    "dotenv-expand": "8.0.3",
    "eslint": "8.34.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-hooks": "0.4.3",
