    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "clean": "rm -rf build/* || true",
    "start": "MONETR_ENV=${MONETR_ENV:=local} webpack serve --hot --mode development",
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom=ui/**/*.{js,jsx,ts,tsx} --coverageDirectory=./reports"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
